{
  "name": "whitebard-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.1.19",
    "react": "^16.7.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.7.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-stripe-elements": "^2.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "yarn run build",
    "deploy": "aws s3 sync build/ s3://whitebard --delete",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E32I4USH2GWPW5 --paths /* && aws cloudfront create-invalidation --distribution-id E1IT6W5W1TC1AE --paths /* ",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
